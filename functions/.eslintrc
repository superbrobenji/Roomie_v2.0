{
    "env": {
        "browser": true,
        "node": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": [
            "error",
            "tab",
            {
                "SwitchCase": 1
            }
        ],
        "linebreak-style": 0,
        "no-tabs": [
            "error",
            {
                "allowIndentationTabs": true
            }
        ],
        "consistent-return": [
            "off"
        ],
        "global-require": "off"
    }
}